{
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".jsx"
        ],
        "alias": {
          "components": "./src/components",
          "hooks": "./src/hooks",
          "utils": "./src/utils.js",
          "propTypes": "./src/propTypes.js"
        }
      }
    ]
  ],
  "exclude": "node_modules/**",
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ]
}